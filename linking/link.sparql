 PREFIX dct: <http://purl.org/dc/terms/>
 PREFIX provo: <http://www.w3.org/ns/prov#>

# brute force Linking
# link everything with the same (lexicalForm|canonicalForm)/writtenRep that does not follow the same URI schema

CONSTRUCT {
  ?a owl:sameAs ?b
} WHERE {
  ?rep ^ontolex:writtenRep ?aform, ?bform.
  FILTER(?aform != ?bform)
  FILTER(isblank(?aform) || isblank(?bform) || strbefore(str(?aform),"#")!= strbefore(str(?bform),'#'))
  ?aform ^(ontolex:lexicalForm|ontolex:canonicalForm) ?a.
  ?bform ^(ontolex:lexicalForm|ontolex:canonicalForm) ?b.
  FILTER(?a != ?b)
  FILTER(strbefore(str(?a),"#")!= strbefore(str(?b),'#'))
}


